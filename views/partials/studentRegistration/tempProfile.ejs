<div class="col-md-3">

    <!-- Profile Image -->
    <div class="box box-primary" ng-if="step1_enable">
        <div class="box-body box-profile">
            <img class="profile-user-img img-responsive img-circle" src="/dist/img/user4-128x128.jpg" alt="User profile picture">

            <h3 class="profile-username text-center">{{firstname + ' '+ middlename + ' ' +lastname}}</h3>

            <p class="text-muted text-center">Indian Institute of Information Technology, Kota</p>

            <ul class="list-group list-group-unbordered">
                <li class="list-group-item">
                    <b>Mobile No</b> <p class="pull-right" >{{mobileno}}</p>
                </li>
                <li class="list-group-item">
                    <b>Date of Birth</b> <p class="pull-right">{{dob}}</p>
                </li>
                <li class="list-group-item">
                    <b>Email</b> <p class="pull-right">{{email}}</p>
                </li>
                <li class="list-group-item">
                    <b>Branch Allotted</b> <p class="pull-right">{{branch}}</p>
                </li>

            </ul>


        </div>
        <!-- /.box-body -->
    </div>
    <!-- /.box -->

    <!-- Profile Box -->
    <div class="box box-primary" ng-if="step2_enable" >
        <div class="box-header with-border">
            <h3 class="box-title">Parent Information</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <strong><i class="fa fa-book margin-r-5"></i>Father's Name</strong>

            <p class="text-muted">
                {{fathername}}
            </p>

            <hr>

            <strong><i class="fa fa-map-marker margin-r-5"></i> Mother's Name</strong>

            <p class="text-muted">{{mothername}}</p>

            <hr>

            <strong><i class="fa fa-map-marker margin-r-5"></i>Father's Mobile</strong>

            <p class="text-muted">{{father_mobileno}}</p>

            <hr>
            <strong><i class="fa fa-map-marker margin-r-5"></i>Mother's Mobile</strong>

            <p class="text-muted">{{mother_mobileno}}</p>
            <hr>

            <strong><i class="fa fa-pencil margin-r-5"></i> Skills</strong>

            <p>
                <span class="label label-danger">UI Design</span>
                <span class="label label-success">Coding</span>
                <span class="label label-info">Javascript</span>
                <span class="label label-warning">PHP</span>
                <span class="label label-primary">Node.js</span>
            </p>

            <hr>

            <strong><i class="fa fa-file-text-o margin-r-5"></i> Notes</strong>

            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Etiam fermentum enim neque.</p>
        </div>
        <!-- /.box-body -->
    </div>
    <!-- /.Profile box -->
</div>