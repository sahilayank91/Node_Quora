<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AskBin | Registration</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!--Image Capture css -->
    <link rel="stylesheet" href="/stylesheets/image_capture.css">
    <!--Including layout files-->
    <% include ../../layout.ejs %>
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="/bower/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/bower/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="/bower/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="/dist/css/skins/_all-skins.min.css">
    <!-- Morris chart -->
    <link rel="stylesheet" href="/bower/morris.js/morris.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="/bower/jvectormap/jquery-jvectormap.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="/bower/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="/bower/bootstrap-daterangepicker/daterangepicker.css">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">


    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]-->
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div ng-app="ASK_BIN" ng-controller="RegistrationController as up">


<div class="wrapper">

    <!--##################################################################################################################-->
    <!--include header here-->
    <% include ../header.ejs %>



    <!-- Left side column. contains the logo and sidebar -->
    <% include ../sidebar.ejs %>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Register Student
                <small>Control panel</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Register Student</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <!-- Small boxes (Stat box) -->
            <div class="row">
                <div class="col-lg-3 col-xs-6" ng-if="step1_enable">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <h3>STEP-1</h3>

                            <p>Student Information</p>
                        </div>
                        <div class="icon">
                            <i class="glyphicon glyphicon-arrow-right"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6" ng-if="step2_enable">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">
                            <h3>STEP-2<sup style="font-size: 20px">%</sup></h3>

                            <p>Parent Information</p>
                        </div>
                        <div class="icon">
                            <i class="glyphicon glyphicon-arrow-right"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6" ng-if="step3_enable">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <h3>STEP-3</h3>

                            <p>Address Verification</p>
                        </div>
                        <div class="icon">
                            <i class="glyphicon glyphicon-arrow-right"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6" ng-if="step4_enable">
                    <!-- small box -->
                    <div class="small-box bg-red">
                        <div class="inner">
                            <h3>STEP-4</h3>

                            <p>Guardian Information</p>
                        </div>
                        <div class="icon">
                            <i class="glyphicon glyphicon-arrow-right"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
            </div>
            <!-- /.row -->
            <!-- Main row -->

            <div class="row">

                <div class="col-lg-9">
            <div class="box box-default" id="step-1" ng-show="step1_enable">
                <div class="box-header with-border">
                  <h3 class="box-title">Step 1 - Student Information</h3>
        

                </div>
                <!-- /.box-header -->
                
                <div class="box-body">

                    <div class="alert alert-danger alert-dismissible" ng-if="error.exists">
                        <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                        {{error.content}}
                    </div>



                    <div class="row">
                    <div class="col-md-12">
                      <div class="form-group">
                       <!-- text input -->
                       
                        <div class="form-group has-feedback">
                            <label>Firstname</label>
                            <input type="text" class="form-control" placeholder="Enter Firstname" ng-model="firstname" ng-required="true">
                        </div>
                        <div class="form-group has-feedback">
                                <label>Middlename</label>
                                <input type="text" class="form-control" placeholder="Enter Middlename" ng-model="middlename">
                            </div>
                        <div class="form-group has-feedback">
                            <label>Lastname</label>
                            <input type="text" class="form-control" placeholder="Enter Lastname" ng-model="lastname" ng-required="true">
                        </div>
                        <div class="form-group has-feedback">
                                <label>Mobile No</label>
                                <input type="text" class="form-control" placeholder="Enter Mobile No" ng-model="mobileno" ng-required="true">
                        </div>

                                    <!-- Date -->
                        <div class="form-group">
                            <label>Date of Birth:</label>

                            <div class="input-group date">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                            <input type="date" class="form-control pull-right" id="datepicker" ng-model="dob" ng-required="true">
                            </div>
                            <!-- /.input group -->
                        </div>
                        <!-- /.form group -->
                        <div class="form-group has-feedback">
                                <label>Email</label>
                                <input type="email" class="form-control" placeholder="Enter Email" ng-model="email" ng-required="true">
                        </div>
                          <div class="form-group has-feedback">
                              <label>Program Allocated</label>
                              <select class="form-control select2" style="width: 100%;" ng-model="branch" ng-required="true">
                                  <option selected="selected">CSE</option>
                                  <option>ECE</option>

                              </select>
                          </div>


                          <div class="form-group">
                                <label>Category</label>
                                <select class="form-control select2" style="width: 100%;"  ng-model="category" ng-required="true">
                                  <option selected="selected">None</option>
                                  <option>General</option>
                                  <option>OBC</option>
                                  <option>SC</option>
                                  <option>ST</option>
                                </select>
                              </div>
                        </div> 
                        <div class="form-group">
                                <label>Gender</label>
                                <select class="form-control select2" style="width: 100%;" ng-model="gender" ng-required="true">
                                  <option selected="selected">Male</option>
                                  <option>Female</option>
                                  <option>Other</option>
                                </select>
                              </div>
                        </div> 

                      
                    </div>
                   
                     
                      <!-- /.form-group -->
                    </div>
                    <!-- /.col -->
                  </div>
                
                <!-- /.box-body -->


            <div class="row" ng-show="step1_enable">

                <div class="col-xs-4 pull-right">
                    <input type="button" name="step-2" id="step2" value="Next" ng-click="step_one_Registration()"
                           class="btn btn-primary btn-block btn-flat" />
                </div>
                <!-- /.col -->
            </div>
                </div>
                <div class="col-lg-3">
                    <div class="box box-primary" ng-if="step1_enable">
                        <div class="box-body box-profile">
                            <img class="profile-user-img img-responsive img-circle" src="/images/default_user.png" alt="User profile picture">

                            <h3 class="profile-username text-center">{{firstname + ' '+ middlename + ' ' +lastname}}</h3>

                            <p class="text-muted text-center">Indian Institute of Information Technology, Kota</p>

                            <ul class="list-group list-group-unbordered">
                                <li class="list-group-item">
                                    <b>Mobile No</b> <p class="pull-right">{{mobileno}}</p>
                                </li>
                                <li class="list-group-item">
                                    <b>Date of Birth</b> <p class="pull-right">{{dob | readableDateFilter}}</p>
                                </li>
                                <li class="list-group-item">
                                    <b>Email</b> <p class="pull-right">{{email}}</p>
                                </li>
                                <li class="list-group-item">
                                    <b>Branch Allotted</b> <p class="pull-right">{{branch}}</p>
                                </li>
                                <li class="list-group-item">
                                    <b>Gender</b> <p class="pull-right">{{gender}}</p>
                                </li>
                                <li class="list-group-item">
                                    <b>Category</b> <p class="pull-right">{{category}}</p>
                                </li>
                            </ul>


                        </div>
                        <!-- /.box-body -->
                    </div>
                </div>


            </div>



<!---------------######################################################################################------------>
        <!--Step 2 starts from here-->
            <div class="row">
                <div class="col-lg-9">
            <div class="box box-default" id="step-2" ng-show="step2_enable">

                <div class="box-header with-border">
                    <h3 class="box-title">Step 2 - Parent's Information</h3>


                </div>
                <div class="alert alert-danger alert-dismissible" ng-if="error.exists">
                    <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                    {{error.content}}
                </div>
                <!-- /.box-header -->

                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <!-- text input -->

                                <div class="form-group has-feedback">
                                    <label>Father's Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Father Name" ng-model="fathername">
                                </div>
                                <div class="form-group has-feedback">
                                    <label>Mother's Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Mother Name" ng-model="mothername">
                                </div>

                                <div class="form-group has-feedback">
                                    <label>Father's Mobile No</label>
                                    <input type="text" class="form-control" placeholder="Enter Father's Mobile No" ng-model="father_mobileno">
                                </div>
                                <div class="form-group has-feedback">
                                    <label>Mother's Mobile No</label>
                                    <input type="text" class="form-control" placeholder="Enter Mother's Mobile No" ng-model="mother_mobileno">
                                </div>

                                <!-- /.form group -->
                                <div class="form-group has-feedback">
                                    <label>Parent's Email</label>
                                    <input type="email" class="form-control" placeholder="Enter Email" ng-model="parent_email">
                                </div>

                            </div>


                        </div>



                    </div>



                    <!-- /.form-group -->
                </div>
                <!-- /.col -->
            </div>
<!----------------------------Step 2 -------------------------------------->
            <div class="row" ng-show="step2_enable">
                <div class="col-xs-4">
                    <input type="button" name="step_2" id="step_2"
                           value="Prev" class="btn btn-warning btn-block btn-flat" ng-click="move_to_previous_step()"/>
                </div>
                <div class="col-xs-4 pull-right">
                    <input type="button" name="step-2" id="step2" value="Next"
                           class="btn btn-primary btn-block btn-flat" ng-click="step_two_Registration()" />
                </div>
                <!-- /.col -->
            </div>

                </div>
                    <div class="col-lg-3">
                        <div class="box box-primary" ng-if="step2_enable" >
                            <div class="box-header with-border">
                                <h3 class="box-title">Parent Information</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body">
                                <strong><i class="fa fa-user margin-r-5"></i>Father's Name</strong>

                                <p class="text-muted">
                                    {{fathername}}
                                </p>

                                <hr>

                                <strong><i class="fa fa-user margin-r-5"></i> Mother's Name</strong>

                                <p class="text-muted">{{mothername}}</p>

                                <hr>

                                <strong><i class="fa fa-mobile-phone margin-r-5"></i>Father's Mobile</strong>

                                <p class="text-muted">{{father_mobileno}}</p>

                                <hr>
                                <strong><i class="fa fa-mobile-phone margin-r-5"></i>Mother's Mobile</strong>

                                <p class="text-muted">{{mother_mobileno}}</p>




                                <hr>

                                <strong><i class="fa fa-envelope margin-r-5"></i> Parent 's Email</strong>

                                <p>{{parent_email}}</p>
                            </div>
                            <!-- /.box-body -->
                        </div>
                    </div>
            </div>




            <!-------------Step 3-------------------------->

            <div class="box box-default" id="step-3" ng-show="step3_enable">
                <div class="box-header with-border">
                    <h3 class="box-title">Step 3 - Address Verification</h3>

                </div>
                <div class="alert alert-danger alert-dismissible" ng-if="error.exists">
                    <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                    {{error.content}}
                </div>
                <!-- /.box-header -->

                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <!-- text input -->

                                <div class="form-group has-feedback">
                                    <label>Permanent Address</label>
                                    <input type="text" class="form-control" placeholder="Enter Permanent Address" ng-model = "permanent_address">
                                </div>
                                <div class="form-group">
                                    <label>Hosteller</label>
                                    <select class="form-control select2" style="width: 100%;" ng-model="hosteller">
                                        <option selected="selected">Yes</option>
                                        <option>No</option>

                                    </select>
                                </div>

                                <div class="form-group has-feedback">
                                    <label>Local Address</label>
                                    <input type="text" class="form-control" placeholder="Enter Local Address" ng-model = "local_address">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.form-group -->
                </div>
                <!-- /.col -->
            </div>


            <!-----------------------------step 3 ---------------------->
            <div class="row" ng-show="step3_enable">
                <div class="col-xs-4">
                    <input type="button" name="step_3" id="step_3"
                           value="Prev" class="btn btn-warning btn-block btn-flat" ng-click="move_to_previous_step()" />
                </div>
                <div class="col-xs-4 pull-right">
                    <input type="button" name="step-3" id="step3" value="Next" ng-click="step_three_Registration()"
                           class="btn btn-primary btn-block btn-flat" />
                </div>
                <!-- /.col -->
            </div>

            <!-----------------------------step 4 ---------------------->



        <div class="row">
            <div class="col-lg-9">
            <div class="box box-default " id="step-4" ng-show="step4_enable">



                <div class="box-header with-border">
                    <h3 class="box-title">Step 4 - Guardian Information</h3>


                </div>
                <div class="alert alert-danger alert-dismissible" ng-if="error.exists">
                    <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                    {{error.content}}
                </div>
                <!-- /.box-header -->

                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <!-- text input -->

                                <div class="form-group has-feedback">
                                    <label>Guardian Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Guardian Name" ng-model="guardian_name">
                                </div>
                                <div class="form-group has-feedback">
                                    <label>Guardian Contact</label>
                                    <input type="text" class="form-control" placeholder="Enter Guardian Contact" ng-model="guardian_phone">
                                </div>
                                <div class="form-group has-feedback">
                                    <label>Guardian Email</label>
                                    <input type="email" class="form-control" placeholder="Enter Guardian Email" ng-model="guardian_email">
                                </div>
                                <div class="form-group has-feedback">
                                    <label>Guardian Address</label>
                                    <input type="text" class="form-control" placeholder="Enter Guardian Address" ng-model="guardian_address">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.form-group -->
                </div>
                <!-- /.col -->




            </div>

            <!-----------------------------step 4 ---------------------->

            <div class="row" ng-show="step4_enable">
                <div class="col-xs-4">
                    <input type="button" name="step_4" id="step_4"
                           value="Prev" class="btn btn-warning btn-block btn-flat"  ng-click="move_to_previous_step()"/>
                </div>
                <div class="col-xs-4 pull-right">
                    <input type="button" name="step-4" id="step4" value="Next" ng-click="step_four_Registration()"
                           class="btn btn-primary btn-block btn-flat" />
                </div>
                <!-- /.col -->
            </div>

            </div>


                <div class="col-lg-3">
                    <div class="box box-primary" ng-if="step4_enable" >
                        <div class="box-header with-border">
                            <h3 class="box-title">Parent Information</h3>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <strong><i class="fa fa-user margin-r-5"></i>Guardian Name</strong>

                            <p class="text-muted">
                                {{guardian_name}}
                            </p>

                            <hr>

                            <strong><i class="fa fa-mobile-phone margin-r-5"></i>Guardian Contact</strong>

                            <p class="text-muted">{{guardian_phone}}</p>
                            <hr>

                            <strong><i class="fa fa-envelope margin-r-5"></i>Guardian's Email</strong>

                            <p>{{guardian_email}}</p>

                            <hr>

                            <strong><i class="fa fa-map-marker margin-r-5"></i>Guardian Address</strong>
                            <p>{{guardian_address}}</p>

                        </div>
                        <!-- /.box-body -->
                    </div>
                </div>
            </div>





            <!-------------------------------Step 5-------------------------------->
            <div class="box box-default" id="step-5" ng-show = "step5_enable">

                <div class="box-header with-border">
                    <h3 class="box-title">Step 5 - Upload Profile Picture</h3>


                </div>

                <!--<div class="alert alert-danger alert-dismissible" ng-if="error.exists">-->
                    <!--<h4><i class="icon fa fa-ban"></i> Alert!</h4>-->
                    <!--{{error.content}}-->
                <!--</div>-->
                <!-- /.box-header -->

                <div class="box-body">
                    <div class="row">
                        <div class="col-sm-4"></div>
                        <div class="col-sm-4" >
                            <form method="POST" name="up.upload_form" encType="multipart/form-data">

                                <input
                                        type="file"
                                        ngf-select
                                        ng-model="up.file"
                                        name="file"
                                        ngf-pattern="'image/*'"
                                        accept="image/*"
                                        ngf-max-size="20MB"
                                />
                                Image thumbnail: <img style="width:100px;" ng-show="!!up.file" ngf-thumbnail="up.file || '/images/default_user.png'"/>
                                <i ng-show="up.upload_form.file.$error.required">*required</i><br>
                                <i ng-show="up.upload_form.file.$error.maxSize">File too large
                                    {{up.file.size / 1000000|number:1}}MB: max 20M</i>
                                <p>{{up.progress}}</p>


                            </form>

                        </div>

                        <div class="col-sm-4"></div>

                    </div>

                    <!-- /.form-group -->
                </div>






                <!-- /.col -->
            </div>

            <div class="row" ng-show="step5_enable">
                <div class="col-xs-4">
                    <input type="button" name="step_4" id="step_4"
                           value="Prev" class="btn btn-warning btn-block btn-flat"  ng-click="move_to_previous_step()"/>
                </div>
                <div class="col-xs-4 pull-right">
                    <input type="button" name="step-4" id="step4" value="Next" ng-click="up.submit()"
                           class="btn btn-primary btn-block btn-flat" />
                </div>
                <!-- /.col -->
            </div>


            <!-------------------------step 5------------------------------------------------>



            <!------------Registration Completion Page-------------------------------------------------------------------->


            <!-- Full Registration Details ----------------------------------------------------------------------->





            <div class="box box-default box-body" ng-show = "completionForm">

                <!-- title row -->
                <div class="row">
                    <div class="col-xs-12">
                        <h2 class="page-header">
                            <i class="fa fa-globe"></i> Submit your Details
                            <small class="pull-right">{{date | readableTimeFilter}}</small>
                        </h2>
                    </div>
                </div>

                 <!--...title row ends....-->


                <div class="row" >

                    <div class="col-lg-9">
                        <p class="lead" style="text-align: center">Personal Information</p>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Firstname:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{firstname}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Middlename:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{middlename}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Lastname:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{lastname}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Name:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{firstname + ' ' + middlename + ' '+  lastname}}</p>
                            </div>
                        </div>
                        <br>

                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Date of birth:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{dob}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Email:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{email}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Phone:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{mobileno}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
                                <strong>Gender:</strong>
                            </div>
                            <div class="col-lg-6">
                                <p>{{gender}}</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-6" style="text-align: center">
